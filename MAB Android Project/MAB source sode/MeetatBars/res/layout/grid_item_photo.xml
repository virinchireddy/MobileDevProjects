<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/bar_member_photo_border_width"
    android:layout_height="@dimen/bar_member_photo_border_height"
    android:layout_marginTop="@dimen/padding_xxsmall"
    android:background="@drawable/black_stoke">
     <ImageView
        android:id="@+id/grid_item_image"
        android:layout_width="@dimen/bar_member_photo_width"
        android:layout_height="@dimen/bar_member_photo_height"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
       />
  
   <ImageView
        android:id="@+id/ivBlocked"
        android:layout_width="@dimen/bar_member_photo_width"
        android:layout_height="@dimen/bar_member_photo_height"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:visibility="gone"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        />
  
 
   
</RelativeLayout>